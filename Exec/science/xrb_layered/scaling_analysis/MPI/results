#!/bin/bash
# Script that self-prints onto itself because why not

for file in output*; do
    echo "# $file $(grep Timestep $file | tail -1)" >> results
done


# output_16nodes.txt Timestep 983 starts with TIME = 0.001036110231 DT = 9.703758801e-07
